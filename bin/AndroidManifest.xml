<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/quiz"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DBHelper"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".User_Details"
            android:label="activity_user__details" >
        </activity>
        <activity
            android:name=".Question"
            android:label="activity_question" >
        </activity>
        <activity
            android:name=".InsertQue"
            android:label="activity_insert_que" >
        </activity>
        <activity
            android:name=".ShowData"
            android:label="activity_show_data" >
        </activity>
        <activity
            android:name=".Score"
            android:label="activity_score" >
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login" >
        </activity>
    </application>

</manifest>
